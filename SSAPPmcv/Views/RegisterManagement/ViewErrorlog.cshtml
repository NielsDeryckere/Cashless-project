@model IEnumerable<models.Errorlog>

@{
    ViewBag.Title = "ViewErrorlog";
}

<div class="navbar-collapse collapse">
    <ul class="nav navbar-nav">
        <li>@Html.ActionLink("New", "New", "RegisterManagement")</li>
        <li>@Html.ActionLink("Show Available Registers", "ShowAvailableRegisters", "RegisterManagement")</li>
        <li>@Html.ActionLink("Show assigned Registers", "ShowAssignedRegisters", "RegisterManagement")</li>

    </ul>


</div>

<h2>ViewErrorlog voor kassa @ViewBag.register</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegisterId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Timestamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Message)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stacktrace)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegisterId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Timestamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Message)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Stacktrace)
        </td>
       
    </tr>
}

</table>
@Html.ActionLink("Go back", "ShowAssignedRegisters")